
public class Circulo extends Figura {
	
	private int raio;
	private double area;
	
	public Circulo (int raio, double area) {
		this.raio = raio;
		this.area = area;
	}
	
	public void setArea(double area) {
		this.area = area;
	}

	public int getRaio() {
		return raio;
	}
	
	public void setRaio(int raio) {
		this.raio = raio;
	}
	
	@Override
	public double area() {
		area = Math.PI * (raio ^ 2);
		return area;
	}

	
	
}
