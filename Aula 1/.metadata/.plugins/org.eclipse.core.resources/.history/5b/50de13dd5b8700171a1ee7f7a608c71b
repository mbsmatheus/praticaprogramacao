
public class Circulo extends Figura {
	
	private int raio;
	private double area;
	
	public Circulo (int raio) {
		this.raio = raio;
	}

	public int getRaio() {
		return raio;
	}
	
	public void setRaio(int raio) {
		this.raio = raio;
	}
	
	@Override
	public double area() {
		area = Math.PI * (Math.pow(raio, 2));
		return area;
	}

	
	
}
